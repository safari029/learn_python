    "import poplib,getpass\n",
    "\n",
    "M = poplib.POP3('localhost')\n",
    "M.user(getpass.getuser())\n",
    "M.pass_(getpass.getpass())\n",
    "numMessages = len(M.list()[1])\n",
    "for i in range(numMessages):\n",
    "    for j in M.retr(i+1)[1]:\n",
    "        print(j)"
